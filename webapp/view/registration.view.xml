<mvc:View
    controllerName="com.applexus.technest.controller.registration"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
>
<ScrollContainer id="_IDGenScrollContainer1" 
width="100%"
height="100%"
vertical="true"
horizontal="true">
<tnt:ToolHeader id="_IDGenToolHeader">
<Button id="_IDGenButton" icon="sap-icon://nav-back"  press="onBackButtonPress" />
	</tnt:ToolHeader>
    
        <IconTabBar id="iconTab" class="iconTabBar">
            <items>
                <IconTabFilter
                    id="filter1"
                    key="info"
                    icon="sap-icon://employee"
                    text="Employee"
                    iconColor="Default"
                    class="filter1" 
                >
                    <core:Fragment fragmentName="com.applexus.technest.fragments.registration_employee" type="XML"/>
                </IconTabFilter>
                <IconTabFilter
                    id="filter2"
                    key="info"
                    icon="sap-icon://home"
                     text="Rental Provider"
                    iconColor="Default"
                    class="filter2"
                >
                    <core:Fragment fragmentName="com.applexus.technest.fragments.registration_rental" type="XML"/>
                </IconTabFilter>
            </items>
        </IconTabBar>
    </ScrollContainer>
</mvc:View>
