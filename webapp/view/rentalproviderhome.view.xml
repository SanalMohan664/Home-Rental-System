<mvc:View
    controllerName="com.applexus.technest.controller.rentalproviderhome"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:t="sap.ui.table"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
>
<ScrollContainer id="_IDGenScrollContainer1" 
width="100%"
height="100%"
vertical="true"
horizontal="true">
<Page id="_IDGenPage1" backgroundDesign="Standard" title="Hi Rental Provider, Welcome to TechNest!"></Page>
<HBox id="_IDGenHBox1" justifyContent="End">
      <Button id="_IDGenButton2"  press="onLogoutPress" icon="sap-icon://log"  class="sapUiMediumMarginEnd"></Button>
      </HBox>
    
        <IconTabBar
            id="iconTabBar"
            class="sapUiNoContentPadding"
            select="onSelect"
        >
            <items>
                <IconTabFilter
                    id="_IDGenIconTabFilter1"
                    text="Post Homes"
                    icon="sap-icon://upload"
                    key="info"
                    class="sapUiLargeMarginEnd"
                >
                    <FlexBox
                        id="_IDGenFlexBox1"
                        width="100%"
                        justifyContent="Center"
                        alignItems="Center"
                        height="100%"
                    >
                        <VBox id="_IDGenVBox2">
                            <FlexBox
                                id="_IDGenFlexBox2"
                                justifyContent="Center"
                                class="sapUiSmallMargin"
                            />

                            <HBox id="_IDGenHBox2">
                                <VBox
                                    id="_IDGenVBox3"
                                    class="sapUiSmallMarginEnd"
                                >
                                    <Label
                                        id="_IDGenLabel2"
                                        text="House/Flat"
                                        required="true"
                                    />

                                    <ComboBox
                                        id="houseorflat"
                                        width="250px"
                                        required="true"
                                        items=""
                                    >
                                        <core:Item
                                            id="House"
                                            text="House"
                                        />
                                        <core:Item
                                            id="Flat"
                                            text="Flat"
                                        />
                                    </ComboBox>
                                </VBox>

                                <VBox id="_IDGenVBox4">
                                    <Label
                                        id="_IDGenLabel3"
                                        text="House/Flat No"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput2"
                                        width="250px"
                                    />
                                </VBox>
                            </HBox>

                            <HBox id="_IDGenHBox3">
                                <VBox
                                    id="_IDGenVBox5"
                                    class="sapUiSmallMarginEnd"
                                >
                                    <Label
                                        id="_IDGenLabel4"
                                        text="House/Flat Name"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput3"
                                        width="250px"
                                    />
                                </VBox>

                                <VBox id="_IDGenVBox6">
                                    <Label
                                        id="_IDGenLabel5"
                                        text="Street"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput4"
                                        width="250px"
                                    />
                                </VBox>
                            </HBox>

                            <HBox id="_IDGenHBox4">
                                <VBox
                                    id="_IDGenVBox7"
                                    class="sapUiSmallMarginEnd"
                                >
                                    <Label
                                        id="_IDGenLabel6"
                                        text="Place"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput5"
                                        width="250px"
                                    />
                                </VBox>

                                <VBox id="_IDGenVBox8">
                                    <Label
                                        id="_IDGenLabel7"
                                        text="District"
                                    />

                                    <Input
                                        id="_IDGenInput6"
                                        editable="false"
                                        value="Thiruvananthapuram"
                                        width="250px"
                                    />
                                </VBox>
                            </HBox>

                            <HBox id="_IDGenHBox5">
                                <VBox
                                    id="_IDGenVBox9"
                                    class="sapUiSmallMarginEnd"
                                >
                                    <Label
                                        id="_IDGenLabel8"
                                        text="Pincode"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput7"
                                        width="250px"
                                    />
                                </VBox>

                                <VBox id="_IDGenVBox10">
                                    <Label
                                        id="_IDGenLabel9"
                                        text="No of Bedrooms"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput8"
                                        width="250px"
                                    />
                                </VBox>
                            </HBox>

                            <HBox id="_IDGenHBox6">
                                <VBox
                                    id="_IDGenVBox11"
                                    class="sapUiSmallMarginEnd"
                                >
                                    <Label
                                        id="_IDGenLabel10"
                                        text="No of Persons"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput9"
                                        width="250px"
                                    />
                                </VBox>

                                <VBox id="_IDGenVBox12">
                                    <Label
                                        id="_IDGenLabel11"
                                        text="Furnishing"
                                        required="true"
                                    />

                                    <ComboBox
                                        id="furnishing"
                                        width="250px"
                                        required="true"
                                        items=""
                                    >
                                        <core:Item
                                            id="F"
                                            text="Full Furnished"
                                        />
                                        <core:Item
                                            id="S"
                                            text="Semi Furnished"
                                        />
                                        <core:Item
                                            id="N"
                                            text="Not Furnished"
                                        />
                                    </ComboBox>
                                </VBox>
                            </HBox>

                            <HBox id="_IDGenHBox8">
                                <VBox
                                    id="_IDGenVBox15"
                                    class="sapUiSmallMarginEnd"
                                >
                                    <Label
                                        id="_IDGenLabel15"
                                        text="Rent Amount"
                                        required="true"
                                    />

                                    <Input
                                        id="_IDGenInput15"
                                        width="250px"
                                    />
                                </VBox>
                                 <VBox id="_IDGenVBox">
                                    <Label
                                        id="_IDGenLabel"
                                        text="Currency"

                                        
                                    />

                                    <Input
                                        id="_IDGenInput"
                                        editable="false"
                                        value="INR"
                                        width="250px"
                                    />
                                </VBox>
                            </HBox>
                            <HBox id="_IDGenHBox9">
                                <VBox id="_IDGenVBox40">
                                    <Label
                                        id="_IDGenLabel17"
                                        text="Description"
                                        required="true"
                                    />
                                    <TextArea
                                        id="_IDGenTextArea1"
                                        maxLength="200"
                                        width="515px"
                                    />
                                </VBox>
                            </HBox>

                            <HBox
                                id="_IDGenVBox19"
                                justifyContent="Center"
                                class="sapUiSmallMarginTop"
                            >
                                <Button
                                    id="_IDGenButton1"
                                    text="Post"
                                    press="onPostPress"
                                />
                            </HBox>
                        </VBox>
                    </FlexBox>
                </IconTabFilter>
                <IconTabFilter
                    id="_IDGenIconTabFilter2"
                    text="View Bookings"
                    icon="sap-icon://detail-view"
                    key="viewBooking"
                >
                    <FlexBox
                        id="_IDGenFlexBox3"
                        justifyContent="Center"
                        class="sapUiSmallMargin"
                    />
                    <Table
                        id="_IDGenTable1"
                        items="{viewBookings>/Bookings}"
                        ariaLabelledBy="title"
                    >
                        <columns>
                            <Column
                                id="_IDGenColumn1"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel21"
                                    text="House No"
                                />
                            </Column>

                            <Column
                                id="_IDGenColumn2"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel22"
                                    text="House Name"
                                />
                            </Column>

                            <Column
                                id="_IDGenColumn3"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel23"
                                    text="Status"
                                />
                            </Column>

                            <Column
                                id="_IDGenColumn4"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel24"
                                    text="Booked by"
                                />
                            </Column>

                            <Column
                                id="_IDGenColumn5"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel25"
                                    text="Phone Number"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn6"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel26"
                                    text="Booking Date"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn7"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel27"
                                    text="Action"
                                />
                                
                            </Column>
                        </columns>

                        <items>
                            <ColumnListItem id="_IDGenColumnListItem1">
                                <cells>
                                    <Text
                                        id="_IDGenText1"
                                        text="{viewBookings>HouseNumber}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText2"
                                        text="{viewBookings>AddressName}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText3"
                                        text="Booked"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText4"
                                        text="{viewBookings>Name}"
                                    />
                                </cells>
                                <cells>
                                    <Text
                                        id="_IDGenText5"
                                        text="{viewBookings>PhoneNumber}"
                                    />
                                </cells>
                                <cells>
                                    <Text
                                        id="_IDGenText6"
                                        text="{ path: 'viewBookings>BookingDate', formatter: '.customFormatter.formatDate'}"
                                    />
                                </cells>
                                <cells>
                                    <Button
                                        id="btn2"
                                        text="Change Booking Status"
                                        press="onchangePress"
                                    />
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </IconTabFilter>
                <IconTabFilter
                    id="_IDGenIconTabFilter3"
                    text="Update House Details"
                    icon="sap-icon://repost"
                    key="updateHouse"
                >
                    <Table
                        id="_IDGenTable2"
                        items="{viewHouses>/Houses}"
                        ariaLabelledBy="title"
                    >
                        <columns>
                            <Column
                                id="_IDGenColumn8"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel28"
                                    text="House/Flat Name"
                                />
                            </Column>

                            <Column
                                id="_IDGenColumn9"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel29"
                                    text="House/Flat Number"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn11"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel31"
                                    text="Room Configuration"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn12"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel32"
                                    text="Maximum Persons"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn13"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel33"
                                    text="Furnishing"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn14"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel34"
                                    text="Rent Amount"
                                />
                            </Column>
                            <Column
                                id="_IDGenColumn10"
                                width="5rem"
                            >
                                <Label
                                    id="_IDGenLabel30"
                                    text="Action"
                                />
                            </Column>
                        </columns>

                        <items>
                            <ColumnListItem id="_IDGenColumnListItem2">
                                <cells>
                                    <Text
                                        id="_IDGenText7"
                                        text="{viewHouses>AddressName}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText9"
                                        text="{viewHouses>HouseNumber}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText10"
                                        text="{viewHouses>Bedrooms}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText11"
                                        text="{viewHouses>NumberOfPersons}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText12"
                                        text="{viewHouses>Furnishing}"
                                    />
                                </cells>

                                <cells>
                                    <Text
                                        id="_IDGenText13"
                                        text="{viewHouses>RentAmount}"
                                    />
                                </cells>

                                <cells>
                                    <Button
                                        id="btn3"
                                        text="Update"
                                        press="onUpdatePress"
                                    />
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </IconTabFilter>
            </items>
        </IconTabBar>
        </ScrollContainer>
</mvc:View>
